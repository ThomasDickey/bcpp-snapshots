AC_REVISION($Revision: 7.10 $)
AC_PREREQ(2.13.20020210)
AC_INIT(c_count.c)
AC_CONFIG_HEADER(config.h:config.hin)

CF_CHECK_CACHE([AC_CANONICAL_SYSTEM])

AC_ARG_PROGRAM
AC_PROG_CC
AC_PROG_INSTALL
CF_PROG_LINT
CF_MAKE_TAGS
AC_PROG_MAKE_SET

CF_ANSI_CC_REQD
AC_CONST

CF_ENABLE_WARNINGS
CF_DISABLE_ECHO
CF_PROG_EXT
CF_DISABLE_LEAKS

AC_STDC_HEADERS
AC_HAVE_HEADERS(stdlib.h getopt.h string.h malloc.h)
AC_HAVE_FUNCS(strchr getopt)
CF_GETOPT_HEADER

###	output makefile and config.h
AC_OUTPUT(makefile,,, cat)
